#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/bin/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules/.pnpm/typescript@4.5.2/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules/.pnpm/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/node_modules:/home/rafaelc/Desktop/projects/javascript/node_modules:/home/rafaelc/Desktop/projects/node_modules:/home/rafaelc/Desktop/node_modules:/home/rafaelc/node_modules:/home/node_modules:/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/packages/interfaces/node_modules/typescript/bin/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/packages/interfaces/node_modules/typescript/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/packages/interfaces/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/bin/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules/.pnpm/typescript@4.5.2/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules/.pnpm/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/temp/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/common/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/node_modules:/home/rafaelc/Desktop/projects/javascript/node_modules:/home/rafaelc/Desktop/projects/node_modules:/home/rafaelc/Desktop/node_modules:/home/rafaelc/node_modules:/home/node_modules:/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/packages/interfaces/node_modules/typescript/bin/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/packages/interfaces/node_modules/typescript/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/packages/interfaces/node_modules:/home/rafaelc/Desktop/projects/javascript/acai/modules/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
